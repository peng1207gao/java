<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
    <class name="com.sgcc.dlsc.entity.po.Datetype" table="DATETYPE">
    	<comment>DATETYPE</comment>
        <!-- single primaryKey -->
	    <id name="sdate" type="string" length="50">
	    	<column name="SDATE">
    	        <comment>SDATE</comment>
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="stype" type="string"  length="50"  not-null="true">
	    	<column name="STYPE">
    	        <comment>STYPE</comment>
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>