<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
    <class name="com.sgcc.dlsc.entity.po.PubBusiCross" table="PUB_BUSI_CROSS">
    	<comment>PUB_BUSI_CROSS</comment>
        <!-- single primaryKey -->
	    <id name="guid" type="string" length="32">
	    	<column name="GUID">
    	        <comment>主键</comment>
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="marketid" type="string"  length="32"  not-null="true">
	    	<column name="MARKETID">
    	        <comment>场景ID</comment>
	    	</column>
    	</property>  
         
	    <property name="tyear" type="int"  length="4"  not-null="true">
	    	<column name="TYEAR">
    	        <comment>年份</comment>
	    	</column>
    	</property>  
         
	    <property name="tmonth" type="int"  length="2"  not-null="true">
	    	<column name="TMONTH">
    	        <comment>月份：1、2、3、4……11、12</comment>
	    	</column>
    	</property>  
         
	    <property name="businessmode" type="string"  length="30"  not-null="false">
	    	<column name="BUSINESSMODE">
    	        <comment>交易方式：跨省送电（双边），跨省受电（双边）</comment>
	    	</column>
    	</property>  
         
	    <property name="businessname" type="string"  length="50"  not-null="false">
	    	<column name="BUSINESSNAME">
    	        <comment>交易名称</comment>
	    	</column>
    	</property>  
         
	    <property name="businesstype" type="string"  length="30"  not-null="false">
	    	<column name="BUSINESSTYPE">
    	        <comment>交易类型：长期交易，短期交易，实时交易</comment>
	    	</column>
    	</property>  
         
	    <property name="electricity1" type="big_decimal"  length="16"  not-null="false">
	    	<column name="ELECTRICITY1">
    	        <comment>组织交易电量单位：万千瓦时</comment>
	    	</column>
    	</property>  
         
	    <property name="price1" type="big_decimal"  length="16"  not-null="false">
	    	<column name="PRICE1">
    	        <comment>组织平均电价(含税)单位：元/MWh</comment>
	    	</column>
    	</property>  
         
	    <property name="electricity2" type="big_decimal"  length="16"  not-null="false">
	    	<column name="ELECTRICITY2">
    	        <comment>完成交易电量</comment>
	    	</column>
    	</property>  
         
	    <property name="price2" type="big_decimal"  length="16"  not-null="false">
	    	<column name="PRICE2">
    	        <comment>完成平均电价(含税)</comment>
	    	</column>
    	</property>  
         
	    <property name="state" type="big_decimal"  length="2"  not-null="true">
	    	<column name="STATE">
    	        <comment>状态,0未发布，1发布，2待审批，3审批通过，4退回。默认为0</comment>
	    	</column>
    	</property>  
         
	    <property name="subjectguid" type="string"  length="32"  not-null="false">
	    	<column name="SUBJECTGUID">
    	        <comment>发布主题ID</comment>
	    	</column>
    	</property>  
         
	    <property name="fillinman" type="string"  length="20"  not-null="true">
	    	<column name="FILLINMAN">
    	        <comment>填报人</comment>
	    	</column>
    	</property>  
         
	    <property name="fillintime" type="date"  not-null="true">
	    	<column name="FILLINTIME">
    	        <comment>填报时间</comment>
	    	</column>
    	</property>  
         
	    <property name="bak1" type="big_decimal"  length="16"  not-null="false">
	    	<column name="BAK1">
    	        <comment>备用1</comment>
	    	</column>
    	</property>  
         
	    <property name="bak2" type="big_decimal"  length="16"  not-null="false">
	    	<column name="BAK2">
    	        <comment>备用2</comment>
	    	</column>
    	</property>  
         
	    <property name="bak3" type="big_decimal"  length="16"  not-null="false">
	    	<column name="BAK3">
    	        <comment>备用3</comment>
	    	</column>
    	</property>  
         
	    <property name="bak4" type="big_decimal"  length="16"  not-null="false">
	    	<column name="BAK4">
    	        <comment>备用4</comment>
	    	</column>
    	</property>  
         
	    <property name="bak5" type="string"  length="32"  not-null="false">
	    	<column name="BAK5">
    	        <comment>备用5</comment>
	    	</column>
    	</property>  
         
	    <property name="bak6" type="string"  length="32"  not-null="false">
	    	<column name="BAK6">
    	        <comment>备用6</comment>
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>