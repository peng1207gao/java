<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
    <class name="com.sgcc.dlsc.entity.po.Testfileupdate" table="TESTFILEUPDATE">
    	<comment>TESTFILEUPDATE</comment>
        <!-- single primaryKey -->
	    <id name="deptid" type="string" length="32">
	    	<column name="DEPTID">
    	        <comment>部门ID</comment>
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="deptname" type="string"  length="32"  not-null="false">
	    	<column name="DEPTNAME">
    	        <comment>部门名称</comment>
	    	</column>
    	</property>  
         
	    <property name="deptnum" type="big_decimal"  length="16"  not-null="false">
	    	<column name="DEPTNUM">
    	        <comment>部门人数</comment>
	    	</column>
    	</property>  
         
	    <property name="marketid" type="string"  length="36"  not-null="false">
	    	<column name="MARKETID">
    	        <comment>市场ID</comment>
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>