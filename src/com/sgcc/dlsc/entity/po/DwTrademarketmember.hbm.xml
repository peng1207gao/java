<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
    <class name="com.sgcc.dlsc.entity.po.DwTrademarketmember" table="DW_TRADEMARKETMEMBER">
    	<comment>DW_TRADEMARKETMEMBER</comment>
        <!-- single primaryKey -->
	    <id name="guid" type="string" length="36">
	    	<column name="GUID">
    	        <comment>GUID</comment>
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="tradeseqid" type="string"  length="36"  not-null="false">
	    	<column name="TRADESEQID">
    	        <comment>交易序列ID</comment>
	    	</column>
    	</property>  
         
	    <property name="participantid" type="string"  length="36"  not-null="false">
	    	<column name="PARTICIPANTID">
    	        <comment>市场成员ID</comment>
	    	</column>
    	</property>  
         
	    <property name="thetype" type="string"  length="36"  not-null="false">
	    	<column name="THETYPE">
    	        <comment>类型</comment>
	    	</column>
    	</property>  
         
	    <property name="marketid" type="string"  length="36"  not-null="false">
	    	<column name="MARKETID">
    	        <comment>市场ID</comment>
	    	</column>
    	</property>  
         
	    <property name="busiunitid" type="string"  length="36"  not-null="false">
	    	<column name="BUSIUNITID">
    	        <comment>业务单元ID</comment>
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>