<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
    <class name="com.sgcc.dlsc.entity.po.SjjcGhCydl" table="SJJC_GH_CYDL">
    	<comment>规划计划_厂用电率</comment>
        <!-- single primaryKey -->
	    <id name="objId" type="string" length="50">
	    	<column name="OBJ_ID">
    	        <comment>记录ID</comment>
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="companyname" type="string"  length="128"  not-null="true">
	    	<column name="COMPANYNAME">
    	        <comment>运行单位名称</comment>
	    	</column>
    	</property>  
         
	    <property name="companyid" type="string"  length="50"  not-null="true">
	    	<column name="COMPANYID">
    	        <comment>运行单位编码</comment>
	    	</column>
    	</property>  
         
	    <property name="provincename" type="string"  length="128"  not-null="true">
	    	<column name="PROVINCENAME">
    	        <comment>省市名称</comment>
	    	</column>
    	</property>  
         
	    <property name="provinceid" type="string"  length="50"  not-null="true">
	    	<column name="PROVINCEID">
    	        <comment>省市编码</comment>
	    	</column>
    	</property>  
         
	    <property name="plantname" type="string"  length="128"  not-null="true">
	    	<column name="PLANTNAME">
    	        <comment>电厂名称</comment>
	    	</column>
    	</property>  
         
	    <property name="plantid" type="string"  length="50"  not-null="false">
	    	<column name="PLANTID">
    	        <comment>电厂ID</comment>
	    	</column>
    	</property>  
         
	    <property name="theyear" type="big_decimal"  length="4"  not-null="false">
	    	<column name="THEYEAR">
    	        <comment>年份</comment>
	    	</column>
    	</property>  
         
	    <property name="themonth" type="big_decimal"  length="2"  not-null="false">
	    	<column name="THEMONTH">
    	        <comment>月份</comment>
	    	</column>
    	</property>  
         
	    <property name="generationquantity" type="big_decimal"  length="16"  not-null="false">
	    	<column name="GENERATIONQUANTITY">
    	        <comment>发电量，单位：MWh</comment>
	    	</column>
    	</property>  
         
	    <property name="gridquantity" type="big_decimal"  length="16"  not-null="false">
	    	<column name="GRIDQUANTITY">
    	        <comment>上网电量，单位：MWh</comment>
	    	</column>
    	</property>  
         
	    <property name="selfconsumptionratio" type="big_decimal"  length="16"  not-null="false">
	    	<column name="SELFCONSUMPTIONRATIO">
    	        <comment>厂用电率</comment>
	    	</column>
    	</property>  
         
	    <property name="dxpdeptcode" type="string"  length="18"  not-null="false">
	    	<column name="DXPDEPTCODE">
    	        <comment>国网公司代码</comment>
	    	</column>
    	</property>  
         
	    <property name="deptCode" type="string"  length="18"  not-null="false">
	    	<column name="DEPT_CODE">
    	        <comment>DEPT_CODE</comment>
	    	</column>
    	</property>  
         
	    <property name="tabYear" type="big_decimal"  length="4"  not-null="false">
	    	<column name="TAB_YEAR">
    	        <comment>TAB_YEAR</comment>
	    	</column>
    	</property>  
         
	    <property name="tabMonth" type="big_decimal"  length="2"  not-null="false">
	    	<column name="TAB_MONTH">
    	        <comment>TAB_MONTH</comment>
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>