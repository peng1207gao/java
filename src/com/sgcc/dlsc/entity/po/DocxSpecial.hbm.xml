<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
    <class name="com.sgcc.dlsc.entity.po.DocxSpecial" table="DOCX_SPECIAL">
    	<comment>DOCX_SPECIAL</comment>
        <!-- single primaryKey -->
	    <id name="appid" type="string" length="36">
	    	<column name="APPID">
    	        <comment>行ID</comment>
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="docid" type="string"  length="20"  not-null="true">
	    	<column name="DOCID">
    	        <comment>文档ID</comment>
	    	</column>
    	</property>  
         
	    <property name="indexnum" type="big_decimal"  length="22"  not-null="true">
	    	<column name="INDEXNUM">
    	        <comment>序号</comment>
	    	</column>
    	</property>  
         
	    <property name="stype" type="string"  length="20"  not-null="true">
	    	<column name="STYPE">
    	        <comment>类型</comment>
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>